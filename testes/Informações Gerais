Código VBA que executa o executavel python

Sub ExecutarPythonExe()
    Dim caminho_executavel As String
    Dim resultado As Double

    ' Caminho completo do executável gerado pelo PyInstaller
    caminho_executavel = "C:\Users\erics\PycharmProjects\pythonProject\dist\testeComEmpacotador.exe"  ' Altere para o caminho correto do seu arquivo executável

    ' Executa o arquivo .exe
    resultado = Shell(caminho_executavel, vbNormalFocus)

    ' Verifica se a execução foi bem-sucedida
    If resultado <> 0 Then
        MsgBox "O programa foi executado com sucesso!", vbInformation
    Else
        MsgBox "Houve um erro ao tentar executar o programa.", vbCritical
    End If
End Sub
